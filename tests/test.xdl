namespace EEE;

struct Item
{
    string      uid;
    string      type;
    time        from_date;
    time        to_date;
    blob        ical;
}

struct Calendar
{
    string      uid;
    time        create_date;
    blob        ical;
    array<Item> items;
}

struct User
{
    string          uid;
    time            create_date;
    blob            ical;
    Calendar        maincal;
    array<Calendar> calendars;
}

servlet Client
{
    struct Date
    {
      int day;
      int month;
      int year;
      int weekday;
    }
    struct Time
    {
      int hour;
      int minute;
    }
    struct DateTime
    {
      Date date;
      Time time;
    }

    DateTime getTime();
    
    User getUserData(string name);
    boolean setUserData(User data);
}

servlet Server
{
    boolean auth(string username, string password);
    array<boolean> getList();
    array<array<boolean>> getListList();
    boolean setList(array<User> list);
}
